# NeuroFi Environment Configuration

# Application
VITE_APP_NAME=NeuroFi
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=AI-Powered Crypto Trading Platform

# API Endpoints
VITE_AI_API_URL=http://localhost:5000/api
VITE_SENTIMENT_API_URL=http://localhost:5001/api
VITE_BINANCE_API_URL=https://api.binance.com/api/v3

# API Keys (Replace with your actual keys)
VITE_BINANCE_API_KEY=your_binance_api_key_here
VITE_BINANCE_SECRET_KEY=your_binance_secret_key_here
VITE_TWITTER_BEARER_TOKEN=your_twitter_bearer_token_here
VITE_TWITTER_API_KEY=your_twitter_api_key_here
VITE_TWITTER_API_SECRET=your_twitter_api_secret_here

# Authentication
VITE_JWT_SECRET=your_jwt_secret_here
VITE_AUTH_COOKIE_NAME=neurofi_auth
VITE_SESSION_TIMEOUT=3600000

# Features Flags
VITE_ENABLE_AI_RECOMMENDATIONS=true
VITE_ENABLE_SENTIMENT_ANALYSIS=true
VITE_ENABLE_RISK_MANAGEMENT=true
VITE_ENABLE_SOCIAL_TRADING=false
VITE_ENABLE_ADVANCED_CHARTS=true
VITE_ENABLE_MOBILE_APP=false
VITE_ENABLE_API_TRADING=false

# Analytics & Monitoring
VITE_GOOGLE_ANALYTICS_ID=your_ga_id_here
VITE_SENTRY_DSN=your_sentry_dsn_here
VITE_HOTJAR_ID=your_hotjar_id_here

# Development
VITE_DEBUG_MODE=false
VITE_MOCK_API=true
VITE_LOG_LEVEL=info

# Production
VITE_CDN_URL=https://cdn.neurofi.com
VITE_API_RATE_LIMIT=100
VITE_CACHE_DURATION=300000

# Social Media
VITE_TWITTER_URL=https://twitter.com/NeuroFiApp
VITE_DISCORD_URL=https://discord.gg/neurofi
VITE_TELEGRAM_URL=https://t.me/neurofi
VITE_GITHUB_URL=https://github.com/neurofi/neurofi

# Support
VITE_SUPPORT_EMAIL=support@neurofi.com
VITE_DOCS_URL=https://docs.neurofi.com
VITE_STATUS_PAGE_URL=https://status.neurofi.com