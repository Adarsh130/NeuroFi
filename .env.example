# Frontend Environment Variables

# API Configuration
VITE_API_BASE_URL=http://localhost:5000/api

# App Configuration
VITE_APP_NAME=NeuroFi
VITE_APP_VERSION=1.0.0

# Features
VITE_ENABLE_ML_FEATURES=true
VITE_ENABLE_SENTIMENT_ANALYSIS=true
VITE_ENABLE_PRICE_PREDICTIONS=true
VITE_ENABLE_AI_RECOMMENDATIONS=true

# WebSocket Configuration
VITE_WS_BASE_URL=wss://stream.binance.com:9443/ws/

# Chart Configuration
VITE_DEFAULT_CHART_INTERVAL=1h
VITE_DEFAULT_CHART_LIMIT=200

# Development
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info